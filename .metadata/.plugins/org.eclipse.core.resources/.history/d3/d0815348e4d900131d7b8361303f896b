package com.example.coursify.view;

public class FragmentSwitcher {
	private static final FragmentSwitcher fragmentSwitcher;

	private FragmentSwitcher() {
		fragmentSwitcher = this;
	}

	public static FragmentSwitcher getFragmentSwitcher() {
		if (fragmentSwitcher == null) {
			return new FragmentSwitcher();
		} else {
			return fragmentSwitcher;
		}
	}
}
